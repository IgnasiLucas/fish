help(read.table)
Cov <- read.table("coverage.txt", colClasses=c("factor","factor","integer"), col.names=c("smpl","run","cov"))
Cov
attach(Cov)
plot(cov[run==1], cov[run==2])
abline(lm(cov[run==2] ~ cov[run==1]), col="red")
mean(cov[run==2])
mean(cov[run==1])
sum(cov[run==1])
sum(cov[run==2])
points(cov[run==1 & smpl=="Undetermined"], cov[run==2 & smpl=="Undetermined"], col="blue")
cov[run==1 & smpl=="Undetermined"] / sum(cov[run==1])
cov[run==2 & smpl=="Undetermined"] / sum(cov[run==2])
m0 <- lm(cov[run==1] ~ cov[run==2])
summary(m0)
man(anova)
help(anova)
m0 <- lm(cov ~ run + smpl)
help(anova)
anova(m0)
summary(m0)
Cov$prop <- numeric(length=length(smpl))
for (i in 1:length(smpl)){
   Cov$prop[i] <- cov[i] / sum(cov[run==run[i]])
}
Cov
sum(prop)
sum(Cov$prop)
detach(Cov)
attach(Cov)
sum(prop)
sum(prop[run==1])
sum(prop[run==2])
m1 <- lm(prop ~ run + smpl)
summary(m1)
anova(m1)
q()
